{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Debug"
    }
  },
  "MySql": {
    "Client": {
      "Server": "localhost",
      "Port": 3306,
      "Database": "steeltoe",
      "Username": "root",
      "Password": "password"
    }
  },
  "Security": {
    "Oauth2": {
      "Client": {
        "JwtKeyUrl": "http://localhost:8080/uaa/token_keys",
        "SaveTokens": true,
        "ValidateCertificates": false
      }
    }
  },
  "ServiceUrl": {
    "MenuServiceUrl": "http://menu-service/menuItems/{id}"
  }
}
